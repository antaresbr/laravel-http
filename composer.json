{
    "name": "antaresbr/laravel-http",
    "description": "An HTTP package for Laravel Framework",
    "homepage": "https://github.com/antaresbr/laravel-http",
    "keywords": [
        "php",
        "laravel",
        "http"
    ],
    "authors": [
        {
            "name": "Eduardo Guerra",
            "email": "eduardo.antares@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7"
    },
    "autoload": {
        "files": [
            "support/helpers.php"
        ],
        "psr-4": {
            "Antares\\Http\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6"
    },
    "autoload-dev": {
        "psr-4": {
            "Antares\\Http\\Tests\\": "tests/"
        }
    }
}
